      "targets": [
        {
          "datasource": {"type": "grafana-postgresql-datasource", "uid": "P40AE60E18F02DE32"},
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT time, plant_id, ec, ph, nitrogen, phosphorus, potassium, array_to_string(array_remove(array[ CASE WHEN ec < 400 THEN 'EC low' ELSE NULL END, CASE WHEN ph < 5.5 THEN 'pH low' WHEN ph > 8.5 THEN 'pH high' ELSE NULL END, CASE WHEN nitrogen < 10 THEN 'N low' WHEN nitrogen > 30 THEN 'N high' ELSE NULL END, CASE WHEN phosphorus < 5 THEN 'P low' WHEN phosphorus > 15 THEN 'P high' ELSE NULL END, CASE WHEN potassium < 10 THEN 'K low' WHEN potassium > 30 THEN 'K high' ELSE NULL END ], NULL), ', ') AS issues FROM sensor_data WHERE plant_id = 3 AND $__timeFilter(time) AND ( ec < 400 OR ph < 5.5 OR ph > 8.5 OR nitrogen < 10 OR nitrogen > 30 OR phosphorus < 5 OR phosphorus > 15 OR potassium < 10 OR potassium > 30 ) ORDER BY time DESC LIMIT 50",
          "refId": "A"
        }
      ],
      "title": "Recent Alerts",
      "type": "table"
