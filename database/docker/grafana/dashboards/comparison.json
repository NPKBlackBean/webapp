      "targets": [
        {
          "datasource": {"type": "grafana-postgresql-datasource", "uid": "P40AE60E18F02DE32"},
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest AS (SELECT plant_id, MAX(time) as max_time FROM sensor_data GROUP BY plant_id), scores AS (SELECT s.plant_id, CASE WHEN s.ec >= 800 THEN 100 WHEN s.ec >= 600 THEN 75 WHEN s.ec >= 400 THEN 50 ELSE 25 END + CASE WHEN s.ph >= 6.5 AND s.ph <= 7.5 THEN 100 WHEN s.ph >= 5.5 AND s.ph <= 8.5 THEN 50 ELSE 25 END + CASE WHEN s.nitrogen >= 30 THEN 100 WHEN s.nitrogen >= 20 THEN 75 WHEN s.nitrogen >= 10 THEN 50 ELSE 25 END + CASE WHEN s.phosphorus >= 15 THEN 100 WHEN s.phosphorus >= 10 THEN 75 WHEN s.phosphorus >= 5 THEN 50 ELSE 25 END + CASE WHEN s.potassium >= 30 THEN 100 WHEN s.potassium >= 20 THEN 75 WHEN s.potassium >= 10 THEN 50 ELSE 25 END as total_score FROM sensor_data s JOIN latest l ON s.plant_id = l.plant_id AND s.time = l.max_time) SELECT plant_id, ROUND((total_score / 5.0)::numeric, 0) as \"Health Score\", RANK() OVER (ORDER BY total_score DESC) as \"Rank\" FROM scores ORDER BY total_score DESC",
          "refId": "A"
        }
      ],
      "title": "Plant Ranking by Health",
      "type": "table"
    },
